<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team11.genshinhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GenshinHelper">
        <activity
            android:name=".ListCharacterActivity"
            android:exported="false" />
        <activity
            android:name=".ListConsumableActivity"
            android:exported="false" />
        <activity
            android:name=".ListArtifactActivity"
            android:exported="false" />
        <activity
            android:name=".ListEnemyActivity"
            android:exported="false" />
        <activity
            android:name=".ListMaterialActivity"
            android:exported="false" />
        <activity
            android:name=".ListWeaponActivity"
            android:exported="false" />
        <activity
            android:name=".ResinTimerEditActivity"
            android:exported="true" />
        <activity
            android:name=".ResinTimerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>