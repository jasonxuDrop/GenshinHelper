<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="@drawable/background_gradient" >

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/action_bar"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-30dp" />

    <TableLayout
        android:layout_width="302dp"
        android:layout_height="937dp"
        android:translationX="-25dp"
        android:translationY="25dp"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="50dp">


        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                layout="@layout/card_template_consumable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </TableRow>

    </TableLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:background="#1C1C1E"
        tools:layout_editor_absoluteX="-4dp"
        tools:layout_editor_absoluteY="791dp"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/ResinNav"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Resin"
            android:translationX="10dp" />

        <Button
            android:id="@+id/SearchNav"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Search"
            android:translationX="300dp" />
    </com.google.android.material.bottomnavigation.BottomNavigationView>

</androidx.constraintlayout.widget.ConstraintLayout>